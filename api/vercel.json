{
    "functions": {
        "api/index.php": {
            "runtime": "vercel.php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/api/index.php",
            "dest": "api/index.php"
        },
        {
            "src": "/style.css",
            "dest": "/public/style.css"
        },
        {
            "src": "/assets/img/1.png",
            "dest": "/public/assets/img/1.png"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
